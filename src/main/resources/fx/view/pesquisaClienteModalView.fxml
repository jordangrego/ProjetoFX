<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="br.com.montreal.fx.controller.PesquisaClienteModalController">
   <children>
      <VBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <TitledPane animated="false" collapsible="false" maxHeight="100.0" minWidth="600.0" text="Pesquisa de Cliente">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <HBox>
                           <children>
                              <Button fx:id="btnPesquisar" mnemonicParsing="false" onAction="#btnPesquisarClick" text="Pesquisar" />
                           </children>
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </HBox>
                     </children>
                  </AnchorPane>
              </content>
            </TitledPane>
            <TableView fx:id="tbvClientes" minHeight="300.0">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
